
<div class="text-center">
    <h1>Все контакты</h1>
</div>
<table class="table table-bordered table-striped table-sm">
    <thead>
        <tr>
            <th>Id</th>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Отчество</th>
            <th>Номер телефона</th>
            <th>Адрес</th>
            <th>Описание</th>
        </tr>
    </thead>
    <tbody>
        @foreach (PhoneBook.Models.Contact c in Model)
        {
            <partial name="_ContactCard" model="c" />
        }
    </tbody>
</table>
<a class="btn btn-primary" asp-action="ContactForm">Добавить контакт</a>
